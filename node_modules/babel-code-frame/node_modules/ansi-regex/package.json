    "cli",
    "color",
    "colors",
    "colour",
    "command-line",
    "console",
    "escape",
    "find",
    "formatting",
    "match",
    "pattern",
    "re",
    "regex",
    "regexp",
    "rgb",
    "shell",
    "string",
    "styles",
    "terminal",
    "test",
    "text",
    "tty",
    "xterm"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "xo": {
    "rules": {
      "guard-for-in": 0,
      "no-loop-func": 0
    }
  }
}
