{
  "name": "css-to-react-native",
  "description": "Convert CSS text to a React Native stylesheet object",
  "license": "MIT",
  "author": "Jacob Parker",
  "homepage": "https://github.com/styled-components/css-to-react-native#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/css-to-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/styled-components/css-to-react-native/issues"
  },
  "version": "3.0.0",
  "main": "index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "rollup ./src/index.js -o index.js --f cjs && babel index.js -o index.js",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "camelize": "^1.0.0",
    "css-color-keywords": "^1.0.0",
    "postcss-value-parser": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^24.9.0",
    "lint-staged": "^6.1.0",
    "prettier": "^1.18.2",
    "rollup": "^1.22.0"
  },
  "keywords": [
    "CSS",
    "React",
    "ReactNative",
    "styled-components",
    "styles"
  ],
  "jest": {
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
